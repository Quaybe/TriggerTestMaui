<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TriggerTestMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:view="clr-namespace:TriggerTestMaui.View"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate view:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="No MVVM"
        ContentTemplate="{DataTemplate view:NoMVVMPage}"
        Route="NoMVVMPage" />

</Shell>
